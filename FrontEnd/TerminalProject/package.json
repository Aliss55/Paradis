{
  "name": "terminal-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:production",
    "start:development": "ng serve --host 0.0.0.0",
    "start:production": "ng serve --configuration production --proxy-config proxy.conf.json",
    "compile:scss": "npx sass --watch ../packages/Light-Theme/blue/theme.scss:./src/assets/themes/light-theme.css ../packages/Dark-Theme/purple/theme.scss:./src/assets/themes/dark-theme.css",
    "compile:scssOnce": "npx sass ../packages/Light-Theme/blue/theme.scss:./src/assets/themes/light-theme.css ../packages/Dark-Theme/purple/theme.scss:./src/assets/themes/dark-theme.css",
    "build": "node -r dotenv/config set-env.js && ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "animate.css": "^4.1.1",
    "dotenv": "^16.4.5",
    "material-icons": "^1.13.12",
    "primeflex": "^3.3.1",
    "primeng": "^17.16.0",
    "quill": "1.3.7",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "17.0.0",
    "@angular-devkit/build-angular": "^17.1.0",
    "@angular/cli": "^17.1.0",
    "@angular/compiler-cli": "^17.1.0",
    "@briebug/jest-schematic": "^2.1.1",
    "@types/jest": "29.5.11",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "typescript": "~5.3.2"
  }
}
